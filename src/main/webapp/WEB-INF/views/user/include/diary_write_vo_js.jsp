<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<script type="text/javascript">

var d_write_full_year;
var d_write_month;
var d_write_date;
var d_write_day;
var d_write_hours;
var d_write_minutes;
var d_write_seconds;

function getD_write_full_year() {
	return d_write_full_year;
}

function setD_write_full_year(year) {
	d_write_full_year = year;
}

function getD_write_month() {
	return d_write_month;
}

function setD_write_month(month) {
	d_write_month = (month + 1);
}

function getD_write_date() {
	return d_write_date;
}

function setD_write_date(date) {
	d_write_date = date;
}

function getD_write_day() {
	return d_write_day;
}

function setD_write_day(day) {
	d_write_day = day;
}

function getD_write_hours() {
	return d_write_hours;
}

function setD_write_hours(hour) {
	d_write_hours = hour;
}

function getD_write_minutes() {
	return d_write_minutes;
}

function setD_write_minutes(minute) {
	d_write_minutes = minute;
}

function getD_write_seconds() {
	return d_write_seconds;
}

function setD_write_seconds(second) {
	d_write_seconds = second;
}

</script>